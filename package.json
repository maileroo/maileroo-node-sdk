{
  "name": "maileroo-sdk",
  "version": "1.0.1",
  "description": "Official Node.js SDK for Maileroo v2 API. Send transactional/marketing emails via Maileroo with a simple and intuitive API.",
  "keywords": [
    "maileroo",
    "email",
    "smtp",
    "transactional",
    "marketing",
    "sdk",
    "api"
  ],
  "license": "MIT",
  "author": "Areeb Majeed",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maileroo/maileroo-node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/maileroo/maileroo-node-sdk/issues"
  },
  "homepage": "https://github.com/maileroo/maileroo-node-sdk#readme",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean": "rimraf dist || rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
